<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.GridPane?>
<GridPane xmlns="http://javafx.com/javafx/8.0.151" xmlns:fx="http://javafx.com/fxml/1">
    <ComboBox fx:id="fileBox" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
    <Button fx:id="recognizeButton" text="OK" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <ImageView fx:id="inputImage" fitHeight="300.0" fitWidth="300.0" GridPane.rowIndex="1" GridPane.columnIndex="0" pickOnBounds="true" preserveRatio="true"/>
    <ImageView fx:id="resultImage" fitHeight="300.0" fitWidth="300.0" GridPane.rowIndex="1" GridPane.columnIndex="1" pickOnBounds="true" preserveRatio="true"/>
</GridPane>